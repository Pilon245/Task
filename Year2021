
#include <iostream>

using namespace std;
bool year(int one, int two){
	while( one < two ){
	}
	if ((one  % 4 == 0 && one % 100 != 0) || (one%400==0)){
		return true;
	}
	else{
		return false;
	}
	one ++;
}


int main()
{   
	int one;
	int two;
	cin >> one >> two;	
	cout << year(one,two);
    
    
    return 0;
}
