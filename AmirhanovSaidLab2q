#include <iostream>

using namespace std;

int main()
{
    std::cout << "Вводить только ЧИСЛА, иначе будет ошибка:" << endl;
    std::cout << "Введите первое число:" << endl;
    double N;
    std::cin >> N ;
    std::cout << "Введите втрое число:" << endl;
    double M;
    std::cin >> M ;
if ( N < M ) {
        std::cout << "Число ";
        std::cout << N;
        std::cout << " меньше,чем ";
        std::cout << M;
    
        } else if (  N > M) {
           std::cout << "Число ";
        std::cout << N;
        std::cout << " больше,чем ";
        std::cout << M;
        } else if ( N == M) {
            std::cout << "Число ";
        std::cout << N;
        std::cout << " равно ";
        std::cout << M;
        }
        
    
    return 0;
}

