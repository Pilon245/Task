#include <iostream>

#include <conio.h>
using namespace std;
 
int main() {
    int a[ ] = { 2,3,4,5,3,5 };
    int G = 0;
    for(int j = 0; j < 5; j++)  
        for(int i = j+1; i < 6; i++) 
            if(a[j] == a[i]) G++;
     cout <<  G << endl; // для проверки точности
    
    return 0;
}


