#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main() {
int i;//искомое число
int k=0;//кол-во элементов в последовательности(равно 0 так как без него выдовало ошибку: переполнение массива!)
int posled[k];//массив последовательности (вводит пользователь)
int ck;//переменная для заполнения массива в цикле
int g;// для поиска числа в массивах
int otv=0;// переменная которая будет хранить в себе кол-во повторов
cout<<"Введите искомое число:  ";
cin>>i;//пользователь ввел искомое число
cout<<"Введите кол-во элементов в последовательности:  ";
cin>>k;//пользователь ввел кол-во элементов в последовательности
cout<<"Введите числа в последовательности:  ";
for (ck=0;ck<k;ck++){
    cin>>posled[ck];//пользователь ввел числа последовательности в массив
}
    for(g=0;g<k;g++)
    if(posled[k]==posled[g])
    otv++;
           
    
    cout<<"Ваше восхительно прекрасное число ";
    cout<<i;
    cout<<" повторяется вот столько раз:  ";
    cout<<otv;
        
    
}    









