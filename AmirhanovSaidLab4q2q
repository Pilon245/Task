#include <iostream>
#include <string>
#include <vector>

using namespace std;


double math(int a,int i){
	vector<int> arr;
	int sum=0;
	for(i=0;i<12345;i++){//������ ����������� ����� �� ���� ���������
	cin>>a;
	if(a!=0){          // ���� �������� ����� �� ����� 0, �� ���� ������
		arr.push_back((int)a);//��������� ����� �� ������������ � ����� �������
		sum += arr[i];
	}
	else if(a==0){
		return sum/i; // ���� ������������ ������ 0,���� ������������� � ���������� ��. ����. �����
	}
}
}

int main(){
	int a=0;
	int i=0;
	cout<<math(a,i);
}


